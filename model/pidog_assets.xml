 <mujocoinclude>
    <compiler inertiafromgeom="true" angle="radian"/> 
    <!-- <size nstack="300000" nuser_geom="1"/> -->
    <option gravity="0 0 -9.81" timestep="0.001" integrator="RK4" iterations="50" noslip_iterations="50"/>
    <!-- <size njmax="3000" nconmax="500" nstack="1000000" /> -->
    <visual>
        <global offwidth="3024" offheight="1680" />
        <quality shadowsize="4096" offsamples="8" />
        <map force="0.1" fogend="5" />
    </visual>
    <default class="main">
        <mesh scale="1.0 1.0 1.0"/>
        <joint limited="true" margin="0.01" armature="0.001" damping="0.05" frictionloss="0.002" solimplimit="0 .9 .01" solreflimit=".02 1" range="-1.57075 1.57075"/>
        <position kp="20.0" ctrlrange="-1.57075 1.57075" gear="1" forcerange="-1 1"/>
        <geom friction="1 0.5 0.01" margin="0.0" gap="0.0"  group="4" contype="1" conaffinity="1"/>
        <default class="collision">
            <geom conaffinity="0" group="4" type="mesh" solref="0.005 1" solimp="0.95 0.99 0.001"/>
        </default>
        <default class="self_collision">
            <geom contype="2" conaffinity="2" group="3" solref="0.005 1" solimp="0.95 0.99 0.001"  rgba="1.0 0.0 0.0 0.8" />
        </default>
        <default class="visual">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.5 0.5 0.5 1" />
        </default>
        <default class="free">
            <joint type="free" limited="false" armature="0" damping="0" />
        </default>
    </default>

    <include file='meshes.xml'/>

    <!-- self collisions -->
    <contact>
        
        <!-- same leg motor -->
        <pair geom1="back_right_leg_a_c0" geom2="back_right_leg_b_motor_c0" condim="3"/>
        <pair geom1="back_right_leg_a_c0" geom2="back_right_leg_b_motor_c1" condim="3"/>
        <pair geom1="back_right_leg_a_c0" geom2="back_right_leg_b_motor_c2" condim="3"/>
        <pair geom1="back_right_leg_a_c0" geom2="back_right_leg_b_motor_c3" condim="3"/>
        
        <pair geom1="front_right_leg_a_c0" geom2="front_right_leg_b_motor_c0" condim="3"/>
        <pair geom1="front_right_leg_a_c0" geom2="front_right_leg_b_motor_c1" condim="3"/>
        <pair geom1="front_right_leg_a_c0" geom2="front_right_leg_b_motor_c2" condim="3"/>
        <pair geom1="front_right_leg_a_c0" geom2="front_right_leg_b_motor_c3" condim="3"/>
        
        <pair geom1="back_left_leg_a_c0" geom2="back_left_leg_b_motor_c0" condim="3"/>
        <pair geom1="back_left_leg_a_c0" geom2="back_left_leg_b_motor_c1" condim="3"/>
        <pair geom1="back_left_leg_a_c0" geom2="back_left_leg_b_motor_c2" condim="3"/>
        <pair geom1="back_left_leg_a_c0" geom2="back_left_leg_b_motor_c3" condim="3"/>

        <pair geom1="front_left_leg_a_c0" geom2="front_left_leg_b_motor_c0" condim="3"/>
        <pair geom1="front_left_leg_a_c0" geom2="front_left_leg_b_motor_c1" condim="3"/>
        <pair geom1="front_left_leg_a_c0" geom2="front_left_leg_b_motor_c2" condim="3"/>
        <pair geom1="front_left_leg_a_c0" geom2="front_left_leg_b_motor_c3" condim="3"/>

        <!-- same side legs  -->
        <pair geom1="back_right_leg_a_c0" geom2="front_right_leg_a_c0" condim="3"/>

        <pair geom1="back_right_acrylic_r_c0" geom2="front_right_leg_a_c0" condim="3"/>
        <pair geom1="back_right_acrylic_l_c0" geom2="front_right_leg_a_c0" condim="3"/>
        <pair geom1="back_right_leg_a_c0" geom2="front_right_acrylic_r_c0" condim="3"/>
        <pair geom1="back_right_leg_a_c0" geom2="front_right_acrylic_l_c0" condim="3"/>
        
        
        <pair geom1="back_left_leg_a_c0" geom2="front_left_leg_a_c0" condim="3"/>
    
        <pair geom1="back_left_acrylic_r_c0" geom2="front_left_leg_a_c0" condim="3"/>
        <pair geom1="back_left_acrylic_l_c0" geom2="front_left_leg_a_c0" condim="3"/>
        <pair geom1="back_left_leg_a_c0" geom2="front_left_acrylic_r_c0" condim="3"/>
        <pair geom1="back_left_leg_a_c0" geom2="front_left_acrylic_l_c0" condim="3"/>


        <!-- <pair geom1="back_left_leg_a_sc" geom2="front_left_leg_a_sc" condim="3"/> -->
    </contact>
   
   <actuator>
        <position joint="front_left_hip"/>
        <position joint="front_left_knee"/>
        <!-- <position joint="front_left_knee" ctrlrange="0.0 1.5708"/> -->

        <position joint="front_right_hip"/>
        <position joint="front_right_knee"/>
        <!-- <position joint="front_right_knee" ctrlrange="-1.5708 0.0"/> -->
        
        <position joint="back_left_hip"/>
        <position joint="back_left_knee"/>
        <!-- <position joint="back_left_knee" ctrlrange="0.0 1.5708"/> -->
        
        <position joint="back_right_hip"/>
        <position joint="back_right_knee"/>
        <!-- <position joint="back_right_knee" ctrlrange="-1.5708 0.0"/> -->
    </actuator>

