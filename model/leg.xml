
<mujoco>
    <compiler angle="radian" coordinate="local" inertiafromgeom="true" settotalmass="0.6"/>
    <default>
        <default class="visual">
            <geom group="2" type="mesh" contype="0" conaffinity="0"/>
        </default>
        <default class="collision">
            <geom group="3" type="mesh" condim="4" solref="0.004 1" solimp="0.9 0.95 0.001 0.5 2"/>
        </default>
    </default>
    <default>
        <joint armature=".0001" damping="0.025" limited="true" solimplimit="0 .8 .03" solreflimit=".02 1" range="-1.57 1.57"/>
        <geom friction="0.9 0.2 0.1" contype="1" conaffinity="1" rgba="0.8 0.6 .4 1" solimp="0.92 0.96 0.002" solref="0.005 1" condim="4"/>
    </default>
    <compiler inertiafromgeom="true" />
    <size nstack="300000" nuser_geom="1"/>
    <option gravity="0 0 -9.81" timestep="0.001" integrator="RK4"/>
    <asset>
        <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
        <mesh name="leg_a" file="leg_a.obj" scale="0.001 0.001 0.001" />
        <mesh name="leg_b" file="leg_b.obj" scale="0.001 0.001 0.001" />
        <mesh name="motor" file="motor.obj" scale="0.001 0.001 0.001" />
        <mesh name="acrylic" file="acrylic.obj" scale="0.001 0.001 0.001" />
    </asset>

    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="2" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="2 2 2" type="plane"/>
        <body name="front_left_hip" pos="0.00 0.0 0.05" >

            <geom name="leg_b" mass="0.00828" mesh="leg_a" rgba="0.26 0.26 0.26 1" class="collision"/>
            <geom mesh="leg_b" rgba="0.26 0.26 0.26 1" class="visual"/>

            <geom name="motor_b" mass="0.009" mesh="motor" rgba="0.0 0.0 0.0 1" pos="0.01315 0.0096 0.0" euler="0 0 1.57075" class="collision"/>
            <geom mesh="motor" rgba="0.0 0.0 0.0 1" pos="0.01315 0.0096 0.0" euler="0 0 1.57075" class="visual"/>

            <joint name="front_left_hip" type="hinge" axis="0 0 1" pos="0.0069 0.0096 0.0"/>
        
            <body name="front_left_knee" pos="0.041 0.0 0.0119" >

                <geom name="leg_a" mass="0.00730" mesh="leg_a" rgba="0.26 0.26 0.26 1" class="collision"/>
                <geom mesh="leg_a" rgba="0.26 0.26 0.26 1" class="visual"/>


                <geom name="motor_a" mass="0.009" mesh="motor" rgba="0.0 0.0 0.0 1" pos="0.01315 0.0096 0.0015" euler="0 0 1.57075" class="collision"/>
                <geom mesh="motor" rgba="0.0 0.0 0.0 1" pos="0.01315 0.0096 0.0015" euler="0 0 1.57075" class="visual"/>

                <geom name="acrylic_r" mass="0.00158" mesh="acrylic" rgba="0.0 0.0 0.0 1" pos="0.0 0.0 0.0" class="collision"/>
                <geom mesh="acrylic" rgba="1.0 1.0 1.0 0.3" pos="0.0 0.0 0.0" class="visual"/>

                <geom name="acrylic_l" mass="0.00158" mesh="acrylic" rgba="0.0 0.0 0.0 1" pos="0.0 0.0 -0.005" class="collision"/>
                <geom mesh="acrylic" rgba="1.0 1.0 1.0 0.3" pos="0.0 0.0 -0.005" class="visual"/>


                <!-- rivet -->
                <geom type="cylinder" size="0.00325 0.0065" rgba="0.0 0.0 0.0 1" pos="0.063 0.008 0.001"/>
                <geom type="cylinder" size="0.00325 0.0065" rgba="0.0 0.0 0.0 1" pos="0.0775 0.005 0.001"/>


                <joint name="front_left_knee" type="hinge" axis="0 0 1" pos="0.00815 0.0096 0.0" range="0.0 2.59"/>
            </body>
        </body>
    </worldbody>
    <actuator>
        <position joint="front_left_hip" kp="20.0" kv="0" ctrlrange="-1.57075 1.57075" forcerange="-0.3 0.3"/>
        <position joint="front_left_knee" kp="20.0" kv="0" ctrlrange="0.0 2.59" forcerange="-0.3 0.3"/>
    </actuator>

</mujoco>
